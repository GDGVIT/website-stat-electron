{
  "name": "website-stats",
  "productName": "GDG Website Stats",
  "version": "1.0.0",
  "description": "Desktop tool to display statistics for website developers",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "watch": "watchify app/app.js -t babelify -o public/js/bundle.js --debug --verbose",
    "postinstall": "install-app-deps",
    "dev": "NODE_ENV='development' npm run start",
    "dist": "build -mwl --x64 --ia32"
  },
  "author": "Rahul A. Krishna",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "chart.js": "^2.3.0",
    "classnames": "^2.2.3",
    "electron-prebuilt": "^1.4.13",
    "electron-reload": "^0.2.0",
    "jquery": "3.0.0",
    "react": "^0.14.8",
    "react-autocomplete": "^1.0.0-rc2",
    "react-circular-progressbar": "^0.1.3",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "electron-builder": "^21.2.0",
    "electron-react-devtools": "^0.5.3"
  },
  "build": {}
}
